{
    "name": "spryker-shop/navigation-widget",
    "type": "library",
    "description": "NavigationWidget module",
    "license": "proprietary",
    "require": {
        "php": ">=7.2",
        "spryker-shop/shop-ui": "^1.0.0",
        "spryker/kernel": "^3.30.0",
        "spryker/navigation-storage": "^1.0.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "SprykerShop\\": "src/SprykerShop/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "abandoned": "spryker-shop/content-navigation-widget"
}
